{
  "name": "byo",
  "version": "2.1.2",
  "description": "byo is like choo without a renderer",
  "main": "index.js",
  "scripts": {
    "build": "mkdir -p dist/ && browserify index -p bundle-collapser/plugin > dist/bundle.js && browserify index -g unassertify -g uglifyify -p bundle-collapser/plugin | uglifyjs > dist/bundle.min.js && zopfli -i 100 dist/bundle.min.js && wc -c < dist/bundle.min.js.gz | pretty-bytes",
    "prepublish": "npm run build",
    "deps": "dependency-check . && dependency-check . --extra --no-dev",
    "test": "standard && npm run deps"
  },
  "repository": "jongacnik/byo",
  "keywords": [
    "choo",
    "client",
    "frontend",
    "framework",
    "minimal",
    "composable",
    "tiny"
  ],
  "author": "Jon Gacnik <jon@folderstudio.com>",
  "license": "MIT",
  "dependencies": {
    "document-ready": "^2.0.1",
    "nanobus": "^3.1.0",
    "nanohistory": "^1.0.0",
    "nanohref": "^1.0.0",
    "nanoraf": "^3.0.1",
    "nanorouter": "^2.0.0"
  },
  "devDependencies": {
    "browserify": "^14.3.0",
    "bundle-collapser": "^1.2.1",
    "dependency-check": "^2.8.0",
    "node-zopfli": "^2.0.2",
    "pretty-bytes": "^5.0.0",
    "pretty-bytes-cli": "^2.0.0",
    "standard": "^10.0.2",
    "uglifyify": "^3.0.4",
    "uglifyjs": "^2.4.10",
    "unassertify": "^2.0.4"
  }
}
